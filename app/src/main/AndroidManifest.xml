<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.usuario.inventoryfragment"
    >

    <application
        android:name="com.example.usuario.inventoryfragment.ui.inventory.InventoryApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name="com.example.usuario.inventoryfragment.ui.splash.SplashActivity"/>
        <activity
            android:name="com.example.usuario.inventoryfragment.ui.login.LoginViewImpl"
            android:theme="@style/LoginTheme"
            >
        </activity>
        <activity android:name="com.example.usuario.inventoryfragment.ui.inventory.InventoryActivity"/>
        <activity android:name="com.example.usuario.inventoryfragment.ui.dashboard.DashboardActivity"/>
        <activity android:name="com.example.usuario.inventoryfragment.ui.product.ProductActivity"/>
        <activity android:name="com.example.usuario.inventoryfragment.ui.dependency.DependencyActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.example.usuario.inventoryfragment.ui.sector.SectorActivity"/>
        <activity android:name="com.example.usuario.inventoryfragment.ui.prefs.GeneralSettingsActivity"/>
        <activity android:name="com.example.usuario.inventoryfragment.ui.prefs.AccountSettingsActivity"/>
        <activity android:name="com.example.usuario.inventoryfragment.ui.about.AboutActivity"/>
    </application>

</manifest>